		"src/index.js",
		"src/store/index.js",
		"build/index.cjs",
		"build/store/index.cjs",
		"build-module/index.mjs",
		"build-module/store/index.mjs"
	],
	"dependencies": {
		"@wordpress/a11y": "^4.38.0",
		"@wordpress/compose": "^7.38.0",
		"@wordpress/data": "^10.38.0",
