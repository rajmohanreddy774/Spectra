		"@wordpress/element": "^6.38.0",
		"@wordpress/escape-html": "^3.38.0",
		"@wordpress/i18n": "^6.11.0",
		"@wordpress/keycodes": "^4.38.0",
		"colord": "2.9.3",
		"memize": "^2.1.0"
	},
	"devDependencies": {
		"deep-freeze": "0.0.1"
